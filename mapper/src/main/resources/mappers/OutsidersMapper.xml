<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cys.hfparking.dao.OutsidersMapper">
  <resultMap id="BaseResultMap" type="com.cys.hfparking.entity.Outsiders">
    <!--
      WARNING - @mbg.generated
    -->
    <id column="outid" jdbcType="INTEGER" property="outid" />
    <result column="out_name" jdbcType="VARCHAR" property="outName" />
    <result column="out_tel" jdbcType="VARCHAR" property="outTel" />
    <result column="out_car" jdbcType="VARCHAR" property="outCar" />
    <result column="visit_purpose" jdbcType="VARCHAR" property="visitPurpose" />
    <result column="id_num" jdbcType="VARCHAR" property="idNum" />
    <result column="liable_person" jdbcType="VARCHAR" property="liablePerson" />
    <result column="remarks" jdbcType="VARCHAR" property="remarks" />
    <result column="entry_time" jdbcType="TIMESTAMP" property="entryTime" />
    <result column="departure_time" jdbcType="TIMESTAMP" property="departureTime" />
  </resultMap>
  <select id="findAllOutsiders" resultMap="BaseResultMap">
    select
    outid,
    out_name,
    out_tel,
    out_car,
    visit_purpose,
    id_num,
    liable_person,
    remarks,
    entry_time,
    departure_time
    from outsiders
    <where>
      <if test="outName!=''and outName!=null and outName!='%%'">
        and out_name like #{outName}
      </if>
      <if test="outCar!=''and outCar!=null and outCar!='%%'">
        and out_car like #{outCar}
      </if>
    </where>
    ORDER BY entry_time desc
    limit #{start},#{limit}
  </select>
</mapper>